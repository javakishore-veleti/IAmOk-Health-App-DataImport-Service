{
    "request": {
      "method": "GET",
      "urlPath": "/api/teacher-profile",
      "queryParameters": {
        "firstName": { "matches": ".*" },
        "lastName": { "matches": ".*" },
        "email": { "matches": ".*" }
      }
    },
    "response": {
      "status": 200,
      "bodyFileName": "teacher-profile-response.json",
      "headers": {
        "Content-Type": "application/json"
      },
      "transformers": ["response-template"]
    }
  }
  